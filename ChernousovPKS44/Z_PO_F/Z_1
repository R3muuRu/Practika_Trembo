using System;

class Program
{
    static int AddNumbers(int a, int b)
    {
        return a + b;
    }

    static void Main()
    {
        int num1 = 5;
        int num2 = 10;

        int sum = AddNumbers(num1, num2);

        Console.WriteLine("Сумма чисел {0} и {1} равна: {2}", num1, num2, sum);
    }
}
